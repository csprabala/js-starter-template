{
  "name": "javascript-starter-template",
  "version": "1.0.0",
  "description": "Javascript Starter Template based on Cory House's Pluralsight course",
  "scripts": {
    "open": "node buildScripts/srcServer.js",
    "start": "npm-run-all lint test open",
    "prestart": "node buildScripts/preStart.js",
    "lint": "npx tslint -p .",
    "test": "npx jest",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint test",
    "build": "node buildScripts/build.js",
    "postbuild": "node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Chandrasekhar Prabala",
  "license": "MIT",
  "dependencies": {
    "patternfly": "^3.54.1",
    "patternfly-demo-app": "git+https://git@github.com/patternfly/patternfly-demo-app.git"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.6",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "npx": "^10.2.0",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "surge": "^0.20.1",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-md5-hash": "0.0.6"
  }
}
